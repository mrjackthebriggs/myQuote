<p style="color: green"><%= notice %></p>

<%= render @quote %>



<div>
  <!--Buttons, so you don't have to see icky hyperlinks-->
  <a href="/your-quotes" class="btn btn-outline-secondary" style="margin-right:20px;">Back to Quotes</a>
  <a href="<%= edit_quote_path(@quote) %>" class="btn btn-warning mr-5">Edit Quote</a>

  <div style="padding-top: 50px">
  <%= button_to "Delete Quote", @quote, method: :delete, class: "btn btn-danger"%>
  </div>
</div>

